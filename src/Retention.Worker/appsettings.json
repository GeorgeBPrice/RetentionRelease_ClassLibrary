{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "Retention.Lib": "Debug"
        }
    },
    "KeepCount": 1,
    "UseLocalData": true,
    "DevOpsDeploy": {
        "ApiBaseUrl": "https://your-devops-api/",
        "ApiKey": "your-api-key",
        "ApiKeyHeader": "X-DevOpsDeploy-ApiKey",
        "SpaceId": "spaces-1",
        "TimeoutSeconds": 30
    },
    "DataFiles": {
        "Projects": "Data/Projects.json",
        "Environments": "Data/Environments.json",
        "Releases": "Data/Releases.json",
        "Deployments": "Data/Deployments.json"
    },
    "IntegrationTestAPI": {
        "ApiBaseUrl": "http://your-test-devops-api/",
        "ApiKey": "your-test-api-key",
        "ApiKeyHeader": "X-DevOpsDeploy-ApiKey",
        "SpaceId": "spaces-1",
        "TimeoutSeconds": 30
    }
}